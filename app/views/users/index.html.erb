





<% @users.each do |user| %>
  <div>
    <p>name : <%= user.name %></p>
    <%= link_to 'show', user_path(user.id) %>
  </div>
<% end %>



<h1>New Book</h1>
<%= form_with model: @book,  local:true do |f| %>
 <h4>title</h4>
 <%= f.text_field :title %>
 <h4>body</h4>
 <%= f.text_area :body %>
 <%= f.submit 'Create Book' %>
 <% end %>
